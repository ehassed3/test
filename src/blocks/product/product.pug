mixin product
  include product.data.pug

  section(class="page-main__product product")
    h2(class="visually-hidden") Таблица товаров
    table(class="product__table")
      tr(class="product__head")
        each head in productHead
          th(class="product__data")= head

      each company in product
        tr(class="product__company")
          each val in company
            td(class="product__data")= val
